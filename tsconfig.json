{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "sourceMap": false,
        "allowJs": false,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "declaration": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true
    },
    "include": [
      "greeter.ts"
    ]
}
